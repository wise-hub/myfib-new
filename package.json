{
  "name": "myfib-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev --turbopack",
    "test": "cross-env NODE_ENV=test next dev --turbopack",
    "prod": "npm run build:prod && npm run start:prod",
    "build:dev": "cross-env NODE_ENV=development next build",
    "build:test": "cross-env NODE_ENV=test next build",
    "build:prod": "cross-env NODE_ENV=production next build",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "start": "npm run start:prod"
  },
  "dependencies": {
    "next": "^15.0.3",        
    "react": "^18.2.0",            
    "react-dom": "^18.2.0"       
  },
  "devDependencies": {
    "@types/next": "^13.1.3",        
    "@types/react": "^18.2.14",    
    "typescript": "^5.2.2",       
    "cross-env": "^7.0.3"
  }
}
